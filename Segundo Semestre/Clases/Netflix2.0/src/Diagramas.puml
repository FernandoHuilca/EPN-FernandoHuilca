@startuml
'https://plantuml.com/class-diagram


Netflix --> "*" Cuenta
Netflix --> "*"Pelicula
Netflix --> "*"Serie
Cuenta --> "1" Suscripcion
Cuenta --> "num Depende de la suscripcion"Perfil
Perfil --> "1" List

class Netflix {
}
enum Suscripcion{
    BASICO
    ESTANDAR
    PREMIUM
    SIN_PAGAR
}
class Pelicula  {
    - nombre: String []
    - genero: String []
}
class Serie {
    - capitulos : int [n]
    - temporadas : int [n]
    - genero: String []
}
class List {
    - peliculasPorVer : Pelicula[n]
    - seriesPorVer : Serie [n]

}


@enduml